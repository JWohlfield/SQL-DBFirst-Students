@model Student

<form>
    <label>Id: </label>
    <input type="number" name="Id" value="@Model.Id" readonly/>
    <br />

    <label>First Name: </label>
    <input type="text" name="FirstName" value="@Model.FirstName" readonly />
    <br />

    <label>Last Name: </label>
    <input type="text" name="LastName" value="@Model.LastName" readonly />
    <br />

    <label>City: </label>
    <input type="text" name="HomeCity" value="@Model.HomeCity" readonly />
    <br />

    <p>Do you wish to delete this entry?</p>
    <input type="submit" value="Yes" />

</form>
<a asp-action="Index" asp-controller="Student">
    <button >No</button>
</a>

